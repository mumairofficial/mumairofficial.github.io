{
  "name": "rahimia-library",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "yarn tailwind:css && vue-cli-service serve",
    "build": "yarn tailwind:css && vue-cli-service build",
    "lint": "vue-cli-service lint",
    "git:release": "rm -rf ../rl-preview && yarn build && cp -r dist ../rl-preview && git add --all && git commit -m \"release preview\" && git push && echo \"PREVIEW RELEASED\" ",
    "tailwind:css": "./node_modules/.bin/tailwind build ./src/assets/styles/main.css -c ./tailwind.js -o ./public/styles/styles.css"
  },
  "dependencies": {
    "register-service-worker": "^1.0.0",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.4",
    "@vue/cli-plugin-eslint": "^3.0.4",
    "@vue/cli-plugin-pwa": "^3.0.4",
    "@vue/cli-service": "^3.0.4",
    "@vue/eslint-config-airbnb": "^3.0.4",
    "node-sass": "^4.9.0",
    "postcss-import": "^12.0.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.0.1",
    "tailwindcss": "^0.6.6",
    "vue-template-compiler": "^2.5.17"
  }
}
